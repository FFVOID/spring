package com.museum.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

import com.museum.service.ExhibitionService;

import lombok.RequiredArgsConstructor;

@Controller
@RequiredArgsConstructor
public class ExhibitionController {

	private final ExhibitionService exService;
	
	@GetMapping(value = "/admin/exhibitionList")
	public String exhibitionList() {
		return "admin/exhibitionList";
	}
	
	@GetMapping(value = "/admin/newEx")
	public String newEx() {
		return "admin/newEx";
	}
}
