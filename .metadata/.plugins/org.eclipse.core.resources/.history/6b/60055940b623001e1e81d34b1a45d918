package com.museum.controller;

import org.springframework.stereotype.Controller;
import org.springframework.ui.Model;
import org.springframework.validation.BindingResult;
import org.springframework.web.bind.annotation.GetMapping;

import com.museum.dto.NewExDto;
import com.museum.dto.NewItemDto;
import com.museum.service.ExhibitionService;

import jakarta.validation.Valid;
import lombok.RequiredArgsConstructor;

@Controller
@RequiredArgsConstructor
public class ExhibitionController {

	private final ExhibitionService exService;
	
	@GetMapping(value = "/admin/newEx")
	public String newEx(Model model) {
		model.addAttribute("newExDto", new NewExDto());
		model.addAttribute("newItemDto" new NewItemDto());
		return "admin/newEx";
	}
	
	@GetMapping(value = "/admin/exhibitionList")
	public String exhibitionList() {
		return "admin/exhibitionList";
	}
	
	
	
}
